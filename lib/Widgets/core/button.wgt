Name		button
Lib		core

Class		Button
Icon		icon_button.gif
DefaultOptions	-text button
DoubleClickCmd	vTcl:set_command
TreeLabel	@vTcl:core:get_widget_tree_label

Function	"Set Text"	vTcl:button:setText
Function	"Set Command"	vTcl:button:setCommand

proc vTcl:button:setText {} {
    global vTcl
    vTcl:set_label $vTcl(w,widget)
}

proc vTcl:button:setCommand {} {
    global vTcl
    vTcl:set_command $vTcl(w,widget)
}
